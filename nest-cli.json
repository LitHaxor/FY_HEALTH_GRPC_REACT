{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/financial_health/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/financial_health/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/financial_health",
  "projects": {
    "financial_health": {
      "type": "application",
      "root": "apps/financial_health",
      "entryFile": "main",
      "sourceRoot": "apps/financial_health/src",
      "compilerOptions": {
        "tsConfigPath": "apps/financial_health/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "calculation": {
      "type": "application",
      "root": "apps/calculation",
      "entryFile": "main",
      "sourceRoot": "apps/calculation/src",
      "compilerOptions": {
        "tsConfigPath": "apps/calculation/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": ["**/*.proto"],
        "watchAssets": true
      }
    }
  }
}
